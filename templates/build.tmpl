{{ define "build" }}
<div class="build">
    {{ template "splash" .Champion }}
    {{ template "map" .Map }}
    <div>
    <div>{{ .Name }}</div>
    <div>{{ .Champion.Name }}</div>
    </div>
    </br>
    <a href="{{ .PermLink }}">PermLink</a>
    <div class="splashList">
        {{ range .Items }}
            {{ template "itemInBuild" . }}
        {{ end }}
    </div>
</div>
{{ end }}


